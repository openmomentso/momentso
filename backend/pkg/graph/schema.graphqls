directive @goField(forceResolver: Boolean, name: String) on INPUT_FIELD_DEFINITION
  | FIELD_DEFINITION

scalar Time

type User {
  id: ID!
  email: String! @goField(forceResolver: true)
}

type TimeEntry {
  id: ID!
  description: String
  createdBy: User!
  createdAt: Time!
  startedAt: Time!
  completedAt: Time
}
