type SignInPayload {
    token: String!
    user: User!
}

type SignUpPayload {
    token: String!
    user: User!
}

extend type Query {
    me: User!
}

extend type Mutation {
    signIn(email: String!, password: String!): SignInPayload!
    signUp(email: String!, password: String!): SignUpPayload!
}